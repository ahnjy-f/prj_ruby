<% @title = "メンバー登録" %>
<h1><%= @title %></h1>

<div id="generic-form">
    <%= form_with model: @member, url: [ :account, @member ] do |f| %>
        <%= render "form", f: f %>
        <div class="buttons">
            <%= f.submit "登録" %>
            <%= link_to "キャンセル", "/account/admin/show" %>
        </div>
    <% end %>
</div>