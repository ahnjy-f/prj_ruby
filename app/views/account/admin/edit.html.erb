<% @title = "職員アカウントの編集" %>
<h1><%= @title %></h1>

<div id="generic-form">
    <%= form_with model: @m, url: @m.id.to_s do |f| %>
        <%= render "form", f:f %>
        <div class="buttons">
            <%= f.submit "更新" %>
            <%= link_to "キャンセル", :account_root %>
        </div>
    <% end %>
</div>